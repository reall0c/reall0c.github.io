+  {%- endfor %}
+  {%- for taxonomy_name in taxonomy_names | sort %}
+  <div class="mb-2 flex flex-wrap">
+    <span class="block-bg mb-1.5 mr-1.5 rounded-lg px-5 py-1.5">{{
+      m_i18n::get_taxonomy_title(key=taxonomy_name, lk=g_lang_k, d=g_trans_d) }}</span>
