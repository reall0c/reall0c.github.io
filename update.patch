+    <div
+      class="mx-2 h-4 w-4 text-[0] [background:var(--url)_center_center_no-repeat] dark:invert"
+      style="--url: url(./icons/{{ type | escape_xml | safe }}.svg);"
+    >
+      {{ type | safe }}
+    </div>
+    <span><strong>{{ title | default(value=type | title) }}</strong></span>
+  </div>
+  <div class="pl-4">{{ body | markdown | safe }}</div>
+</div>
diff --git a/themes/linkita/templates/shortcodes/gallery.html b/themes/linkita/templates/shortcodes/gallery.html
